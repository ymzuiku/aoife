{
  "name": "aoife",
  "version": "1.7.8",
  "main": "lib/index.ts",
  "types": "aoife.d.ts",
  "exports": {
    "import": "./lib/index.ts"
  },
  "private": false,
  "scripts": {
    "lint": "eslint --ext .tsx,.ts --fix ./lib",
    "cp_readme":"cp -rf ../README.md ./README.md && cp -rf ../README.md ../create-aoife-app/README.md",
    "es": "yarn cp_readme && esbuild --define:process.env.NODE_ENV=\\\"production\\\" lib/index.ts --outdir=es --bundle --format=esm --jsx-factory=aoife --jsx-fragment=aoife.Frag --minify --splitting"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./{lib}/**/*.{ts,tsx}": [
      "npm run lint",
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "@typescript-eslint/typescript-estree": "^4.22.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jsx-control-statements": "^2.2.1",
    "eslint-plugin-prettier": "^3.1.4",
    "fs-extra": "^9.1.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ymzuiku/aoife.git"
  },
  "bugs": {
    "url": "git+https://github.com/ymzuiku/aoife.git"
  },
  "dependencies": {
    "vanilla-life": "^0.1.5"
  }
}
